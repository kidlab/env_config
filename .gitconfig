[user]
	name = Man Vuong
	email = thanhman.gm@gmail.com
[alias]
  ci = commit
  co = checkout
  st = status --short
  lg = log --oneline --decorate -n 20
  wd = diff --word-diff
  wdc = diff --word-diff --cached
  dc = diff --cached
[github]
  user = kidlab
[color]
  diff = auto
  status = auto
[color "status"]
  changed = yellow
  added = green
  untracked = red
[mergetool]
  keepBackup = false
[core]
  editor = /usr/bin/vim
	excludesfile = /Users/manvuong/.gitignore_global
	ignorecase = false
[diff]
	tool = opendiff
[merge]
	tool = opendiff
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
